package learning_java;

import java.util.Scanner;



 enum Month {
	january,
	feburary,
	march,
	april,
	may,
	june,
	july,
	august,
	september,
	october,
	november,
	december;
	

}
public class CheckDaysInMonth {

	public static void main(String[] args)   {
   Scanner take= new Scanner(System.in);
	
	String  month_name =take.nextLine();
	
	
		

	Month month=Month.valueOf(month_name);
	
	month.name();
   System.out.println("the number of the days in ");

  take.close();


		
	
	
	}
}
